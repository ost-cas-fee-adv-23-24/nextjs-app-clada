{
  "name": "mumble",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier . --write",
    "test": "npm-run-all test:*",
    "test:mock": "mocks-server --no-plugins.inquirerCli.enabled",
    "test:playwright": "npx playwright test",
    "test:ui-mode": "npx playwright test --ui"
  },
  "lint-staged": {
    "*.{css,mdx,json,ts,tsx}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --max-warnings 0"
    ]
  },
  "dependencies": {
    "autoprefixer": "10.4.15",
    "eslint": "8.49.0",
    "eslint-config-next": "^14.0.4",
    "next": "^14.0.4",
    "next-auth": "^5.0.0-beta.4",
    "postcss": "8.4.29",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@mocks-server/main": "^4.1.0",
    "@playwright/test": "^1.41.1",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.11"
  }
}
